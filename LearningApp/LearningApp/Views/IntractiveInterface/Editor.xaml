<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LearningApp.Views.IntractiveInterface.Editor">
    <StackLayout>
       
            <SearchBar x:Name="search" SearchButtonPressed="search_SearchButtonPressed" TextChanged="search_TextChanged" />
        <AbsoluteLayout VerticalOptions="FillAndExpand">
            <Editor x:Name="editor" Keyboard="Text" AbsoluteLayout.LayoutBounds="0, 0, 0.8, 0.8" AbsoluteLayout.LayoutFlags="All" Focused="OnEditorFocused" Unfocused="OnEditorUnfocused" />
        </AbsoluteLayout>
    </StackLayout>
</ContentPage>